		// Распределяем Сумма из Cальдо96 (Организация*, Счет, ОбъектИДП**, Сумма) 
		// по Сумма из РезервыОИДП (Организация*, ОбъектИДП**, Корпус, Сумма)
		ТабСальдо96     = РезСальдо96.Выгрузить();
		ТабРезервыОИДП  = РезРезервыОИДП.Выгрузить();
		Таб96ПоКорпусам = ТабРезервыОИДП.СкопироватьКолонки();
		
		Аналитика = Новый Структура("Организация, ОбъектИДП");
		ТабРезервыОИДП.Индексы.Добавить("Организация, ОбъектИДП");
		
		Для Каждого Строка96 Из ТабСальдо96 Цикл
			ЗаполнитьЗначенияСвойств(Аналитика, Строка96);
			МассивКорпусов = ТабРезервыОИДП.НайтиСтроки(Аналитика);
			Коэффициенты   = Новый Массив;
			
			Если МассивКорпусов.Количество() = 0 Тогда
				Продолжить;
			КонецЕсли;
			
			Для Каждого Строка из МассивКорпусов Цикл
				Коэффициенты.Добавить(Строка.Сумма);
			КонецЦикла;
			
			МассивСумм = ОбщегоНазначения.РаспределитьПропорционально(Строка96.Сумма, Коэффициенты);
			Индекс     = 0;
			
			Если МассивСумм = Неопределено Тогда
				ОбщегоНазначения.СообщитьОбОшибке("Не удалось распределить сальдо счета 96 по корпусам (" + Аналитика.ОбъектИДП + ")");
				
			Иначе
				Для Каждого Сумма Из МассивСумм Цикл
					Строка = Таб96ПоКорпусам.Добавить();
					ЗаполнитьЗначенияСвойств(Строка, Строка96);
					Строка.Корпус = МассивКорпусов[Индекс].Корпус;
					Строка.Сумма  = Сумма;
					
					Индекс = Индекс + 1;
				КонецЦикла;
			КонецЕсли;
		КонецЦикла;
		
		// Распределяем Сумма из 96ПоКопусам (Организация*, Счет, ОбъектИДП, Корпус**, Сумма) 
		// по Площадь из ПроданныеОН (Организация*, ОбъектИДП**, ОбъектНедвижимости, ВидОбъектаНедвижимости, Площадь)
		ТабПроданныеОН = РезПроданныеОН.Выгрузить();
		Таб96ПоОН      = ТабПроданныеОН.СкопироватьКолонки();
		
		ТабПроданныеОН.Индексы.Добавить("Организация, ОбъектИДП");
		
		Для Каждого СтрокаКорпуса Из Таб96ПоКорпусам Цикл
			Аналитика.Организация = СтрокаКорпуса.Организация;
			Аналитика.ОбъектИДП   = СтрокаКорпуса.Корпус;
			
			МассивОН     = ТабПроданныеОН.НайтиСтроки(Аналитика);
			Коэффициенты = Новый Массив;
			
			Если МассивОН.Количество() = 0 Тогда
				Продолжить;
			КонецЕсли;
			
			Для Каждого Строка Из МассивОН Цикл
				Коэффициенты.Добавить(Строка.Площадь);
			КонецЦикла;
			
			МассивСумм = ОбщегоНазначения.РаспределитьПропорционально(СтрокаКорпуса.Сумма, Коэффициенты);
			Индекс     = 0;
			
			Если МассивСумм = Неопределено Тогда
				ОбщегоНазначения.СообщитьОбОшибке("Не удалось распределить сальдо счета 96 по объектам недвижимости (" + Аналитика.ОбъектИДП + ")");
				
			Иначе
				Для Каждого Сумма Из МассивСумм Цикл
					Строка = Таб96ПоОН.Добавить();
					ЗаполнитьЗначенияСвойств(Строка, СтрокаКорпуса);
					ЗаполнитьЗначенияСвойств(Строка, МассивОН[Индекс], "ОбъектНедвижимости, ВидОбъектаНедвижимости");
					Строка.Сумма = Сумма;
					
					Индекс = Индекс + 1;
				КонецЦикла;
			КонецЕсли;
		КонецЦикла;
		
		// Формируем проводки (Организация*, ПодразделениеКт, СчетКт, СубконтоКт{ВидОбъектаНедвижимости, Номенклатура}, Сумма, СуммаНУДт) 
		// из Таб96ПоОН (Организация, Счет, ОбъектИДП, Корпус, ОбъектНедвижимсоти, ВидОбъектаНедвижимости, Сумма)
		ВыборкаДанных = Новый Массив;
		
		ОперацияБух = Новый Структура;
		ОперацияБух.Вставить("Организация" , Параметры.Организация);
		ОперацияБух.Вставить("Содержание"  , "#Остатки_РезервыПоИДП_Счет96");
		ОперацияБух.Вставить("Дата"        , Параметры.ДатаОстатков - 1);
		
		ТабПроводки = Новый ТаблицаЗначений;
		ТабПроводки.Колонки.Добавить("СчетДт");
		ТабПроводки.Колонки.Добавить("СчетКт");
		ТабПроводки.Колонки.Добавить("Сумма");
		ТабПроводки.Колонки.Добавить("СуммаВРКт");
		ТабПроводки.Колонки.Добавить("Организация");	
		ТабПроводки.Колонки.Добавить("СубконтоКт");	
		ТабПроводки.Колонки.Добавить("Период");	
		ТабПроводки.Колонки.Добавить("ПодразделениеКт");
		
		ТабДвижения = Новый ТаблицаЗначений;
		ТабДвижения.Колонки.Добавить("ВидОбъектаНедвижимости");
		ТабДвижения.Колонки.Добавить("ОбъектНедвижимости");
		ТабДвижения.Колонки.Добавить("ВидОбъектаИДП");
		ТабДвижения.Колонки.Добавить("ОбъектИДП");
		ТабДвижения.Колонки.Добавить("СчетУчетаБУ");
		ТабДвижения.Колонки.Добавить("Организация");
		ТабДвижения.Колонки.Добавить("Сумма");
		ТабДвижения.Колонки.Добавить("Активность");
		ТабДвижения.Колонки.Добавить("ВидДвижения");
		ТабДвижения.Колонки.Добавить("Период");
		
		СубконтоНоменклатура = ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.Номенклатура;

		СубконтоВидОбъектаИДП = Новый Структура;
		СубконтоВидОбъектаИДП.Вставить("Код", "00067");
		
		СубконтоВидОбъектаНедвижимости = Новый Структура;
		СубконтоВидОбъектаНедвижимости.Вставить("Код", "00068");
		
		ТабИсточник = Таб96ПоОН.Скопировать(, "Организация, Счет, Корпус, ОбъектНедвижимости, ВидОбъектаНедвижимости, Сумма");
		ТабИсточник.Свернуть("Организация, Счет, Корпус, ОбъектНедвижимости, ВидОбъектаНедвижимости", "Сумма");
		
		Для Каждого Строка Из ТабИсточник Цикл
			СубконтоКт = Новый Соответствие;
			СубконтоКт.Вставить(СубконтоВидОбъектаИДП         , ОбщегоНазначения.ЗначениеРеквизитаОбъекта(Строка.Корпус, "бит_ВидОбъекта"));
			СубконтоКт.Вставить(СубконтоВидОбъектаНедвижимости, Строка.ВидОбъектаНедвижимости);
			СубконтоКт.Вставить(СубконтоНоменклатура          , Строка.ОбъектНедвижимости);
			
			Проводка = ТабПроводки.Добавить();
			Проводка.СчетДт          = "00";
			Проводка.СчетКт          = Строка.Счет;
			Проводка.Сумма           = Строка.Сумма;
			Проводка.СуммаВРКт       = Строка.Сумма;
			Проводка.Организация     = Строка.Организация;
			Проводка.СубконтоКт      = СубконтоКт;
			Проводка.Период          = ОперацияБух.Дата;
			Проводка.ПодразделениеКт = Строка.Корпус;
			
			Запись = ТабДвижения.Добавить();
			Запись.ВидОбъектаНедвижимости = Строка.ВидОбъектаНедвижимости;
			Запись.ОбъектНедвижимости     = Строка.ОбъектНедвижимости;
			Запись.ВидОбъектаИДП          = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(Строка.Корпус, "бит_ВидОбъекта");
			Запись.ОбъектИДП              = Строка.Корпус;
			Запись.СчетУчетаБУ            = Строка.Счет;
			Запись.Организация            = Строка.Организация;
			Запись.Сумма                  = Строка.Сумма;
			Запись.Активность             = Истина;
			Запись.ВидДвижения            = ВидДвиженияНакопления.Приход;
			Запись.Период                 = ОперацияБух.Дата;
		КонецЦикла;
			
		//ТабИсточник = Таб96ПоОН.Скопировать(, "Организация, Корпус, Сумма");
		//ТабИсточник.Свернуть("Организация, Корпус", "Сумма");
		//
		//Для Каждого Строка Из ТабИсточник Цикл
		//	Запись = ТабДвижения.Добавить();
		//	Запись.ВидОбъектаИДП = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(Строка.Корпус, "бит_ВидОбъекта");
		//	Запись.ОбъектИДП     = Строка.Корпус;
		//	Запись.Организация   = Строка.Организация;
		//	Запись.Сумма         = Строка.Сумма;
		//	Запись.Активность    = Истина;
		//	Запись.ВидДвижения   = ВидДвиженияНакопления.Приход;
		//	Запись.Период        = ОперацияБух.Дата;
		//КонецЦикла;
		
		ОперацияБух.Вставить("Хозрасчетный"    , ТабПроводки);
		ОперацияБух.Вставить("пик_РезервыПоИДП", ТабДвижения);
		ВыборкаДанных.Добавить(ОперацияБух);
	КонецЕсли;	
КонецЕсли;</ПередОбработкойПравила>
