|	ВТНДСпоОИДП.ОбъектНедвижимости = ЗНАЧЕНИЕ(Справочник.бит_ОбъектыНедвижимости.ПустаяСсылка)
|;
|
|////////////////////////////////////////////////////////////////////////////////
|ВЫБРАТЬ
|	ВТСебестоимостьОИДП.Организация КАК Организация,
|	ВТСебестоимостьОИДП.ОбъектИДП КАК ОбъектИДП,
|	ВТСебестоимостьОИДП.СчетУчетаБУ.Код КАК СчетУчетаБУ,
|	ВТСебестоимостьОИДП.СтатьяЗатрат КАК СтатьяЗатрат,
|	ВТСебестоимостьОИДП.ОбъектНедвижимости КАК ОбъектНедвижимости,
|	ЕСТЬNULL(ВТСебестоимостьОИДП.ОбъектНедвижимости.Владелец, ЗНАЧЕНИЕ(Справочник.ОбъектыСтроительства.ПустаяСсылка)) КАК ВладелецОбъектаНедвижимости,
|	ВТСебестоимостьОИДП.СуммаБезНДС КАК СуммаБезНДС,
|	ВТСебестоимостьОИДП.НДС КАК НДС
|ИЗ
|	ВТСебестоимостьОИДП КАК ВТСебестоимостьОИДП";

Запрос = Новый Запрос(ТекстЗапроса);
Запрос.УстановитьПараметр("ДатаОстатков"          , Параметры.ДатаОстатков);
Запрос.УстановитьПараметр("Организация"           , Параметры.Организация);
Запрос.УстановитьПараметр("БезОтбораПоПлощадкеИДП", Параметры.ОтборПоПлощадкеИДП.Пустая());
Запрос.УстановитьПараметр("ПлощадкаИДП"           , Параметры.ОтборПоПлощадкеИДП);
Запрос.УстановитьПараметр("Счет36"                , ПланыСчетов.Хозрасчетный.НайтиПоКоду("36"));
Запрос.УстановитьПараметр("Счет37"                , ПланыСчетов.Хозрасчетный.НайтиПоКоду("37"));
Запрос.УстановитьПараметр("Счет43_03"             , ПланыСчетов.Хозрасчетный.НайтиПоКоду("43.03"));

Результаты = Запрос.ВыполнитьПакет();

ТабНДСпоОИДПсОН      = Результаты[Результаты.Количество() - 3].Выгрузить(); // итогова таблица
ТабНДСпоОИДПБезОН    = Результаты[Результаты.Количество() - 2].Выгрузить();
ТабСебестоимостьОИДП = Результаты[Результаты.Количество() - 1].Выгрузить();

// вычтем ТабНДСпоОИДПсОН из СебестоимостьОИДП
ИменаРесурсов = Новый Массив;
ИменаРесурсов.Добавить("СуммаБезНДС");
ИменаРесурсов.Добавить("НДС");

ИменаПолейСоединения = "Организация, ОбъектИДП, СчетУчетаБУ, ОбъектНедвижимости";
ОтборСоединения      = Новый Структура(ИменаПолейСоединения);
ТабСебестоимостьОИДП.Индексы.Добавить(ИменаПолейСоединения);

ИменаПолейСоединения = "Организация, ОбъектИДП, СчетУчетаБУ, ОбъектНедвижимости, СтатьяЗатрат";
ОтборСоединения36    = Новый Структура(ИменаПолейСоединения);
ТабСебестоимостьОИДП.Индексы.Добавить(ИменаПолейСоединения);

Для Каждого СтрокаСОН Из ТабНДСпоОИДПсОН Цикл
	Если Лев(СтрокаСОН.СчетУчетаБУ, 6) = "36.03." Тогда
		ЗаполнитьЗначенияСвойств(ОтборСоединения36, СтрокаСОН);
		МассивСР = ТабСебестоимостьОИДП.НайтиСтроки(ОтборСоединения36);
		
	Иначе
		ЗаполнитьЗначенияСвойств(ОтборСоединения, СтрокаСОН);
		МассивСР = ТабСебестоимостьОИДП.НайтиСтроки(ОтборСоединения);
	КонецЕсли;
	
	Если МассивСР.Количество() &gt; 0 Тогда
		Для Каждого ИмяРесурса Из ИменаРесурсов Цикл
			Если СтрокаСОН[ИмяРесурса] = 0 Тогда
				Продолжить;
			КонецЕсли;
			
			Коэффициенты = Новый Массив;
			
			Для Каждого СтрокаСР Из МассивСР Цикл
				Коэффициенты.Добавить(СтрокаСР[ИмяРесурса]);
			КонецЦикла;
			
			Суммы = ОбщегоНазначения.РаспределитьПропорционально(СтрокаСОН[ИмяРесурса], Коэффициенты);
			
			Если Суммы = Неопределено Тогда
				Если Лев(СтрокаСОН.СчетУчетаБУ, 5) = "36.03." Тогда
					ОбщегоНазначения.СообщитьОбОшибке("Не удалось вычесть НДСпоОИДПсОН из СебестоимостьОИДП: "
						+ ОтборСоединения36.Организация  + ", "
						+ ОтборСоединения36.ОбъектИДП    + ", "
					    + ОтборСоединения36.СчетУчетаБУ  + ", "
					    + ОтборСоединения36.СтатьяЗатрат + ", "
					    + ОтборСоединения36.ИмяРесурса   + ", ");
						
				Иначе
					ОбщегоНазначения.СообщитьОбОшибке("Не удалось вычесть НДСпоОИДПсОН из СебестоимостьОИДП: "
						+ ОтборСоединения.Организация  + ", "
						+ ОтборСоединения.ОбъектИДП    + ", "
					    + ОтборСоединения.СчетУчетаБУ  + ", "
					    + ОтборСоединения.ИмяРесурса   + ", ");
				КонецЕсли;
				
				Продолжить;
			КонецЕсли;
			
			Индекс = 0;
			
			Для Каждого СтрокаСР Из МассивСР Цикл
				СтрокаСР[ИмяРесурса] = СтрокаСР[ИмяРесурса] - Суммы[Индекс];
				
				Индекс = Индекс + 1;
			КонецЦикла;
		КонецЦикла;
	КонецЕсли;
КонецЦикла;

// распределяем НДСпоОИДПБезОН по СебестоимостьОИДП
ПустыеРесурсы = Новый Структура("СуммаБезНДС, НДС", 0, 0);

ИменаПолейСоединения = "Организация, ОбъектИДП, СчетУчетаБУ";
ОтборСоединения      = Новый Структура(ИменаПолейСоединения);
ТабСебестоимостьОИДП.Индексы.Добавить(ИменаПолейСоединения);

ИменаПолейСоединения = "Организация, ОбъектИДП, СчетУчетаБУ, СтатьяЗатрат";
ОтборСоединения36    = Новый Структура(ИменаПолейСоединения);
ТабСебестоимостьОИДП.Индексы.Добавить(ИменаПолейСоединения);

Для Каждого СтрокаБезОН Из ТабНДСпоОИДПБезОН Цикл
	Если Лев(СтрокаБезОН.СчетУчетаБУ, 6) = "36.03." Тогда
		ЗаполнитьЗначенияСвойств(ОтборСоединения36, СтрокаБезОН);
		МассивСР = ТабСебестоимостьОИДП.НайтиСтроки(ОтборСоединения36);
		
	Иначе
		ЗаполнитьЗначенияСвойств(ОтборСоединения, СтрокаБезОН);
		МассивСР = ТабСебестоимостьОИДП.НайтиСтроки(ОтборСоединения);
	КонецЕсли;
	
	Если МассивСР.Количество() = 0 Тогда
		ЗаполнитьЗначенияСвойств(ТабНДСпоОИДПсОН.Добавить(), СтрокаБезОН);
		
	Иначе
		Для Каждого ИмяРесурса Из ИменаРесурсов Цикл
			Если СтрокаБезОН[ИмяРесурса] = 0 Тогда
				Продолжить;
			КонецЕсли;
			
			Коэффициенты = Новый Массив;
			
			Для Каждого СтрокаСР Из МассивСР Цикл
				Коэффициенты.Добавить(СтрокаСР[ИмяРесурса]);
			КонецЦикла;
			
			Суммы = ОбщегоНазначения.РаспределитьПропорционально(СтрокаБезОН[ИмяРесурса], Коэффициенты);
			
			Если Суммы = Неопределено Тогда
				Если Лев(СтрокаБезОН.СчетУчетаБУ, 5) = "36.03." Тогда
					ОбщегоНазначения.СообщитьОбОшибке("Не удалось распределить НДСпоОИДПБезОН по СебестоимостьОИДП: "
						+ ОтборСоединения36.Организация  + ", "
						+ ОтборСоединения36.ОбъектИДП    + ", "
					    + ОтборСоединения36.СчетУчетаБУ  + ", "
					    + ОтборСоединения36.СтатьяЗатрат + ", "
					    + ОтборСоединения36.ИмяРесурса   + ", ");
						
				Иначе
					ОбщегоНазначения.СообщитьОбОшибке("Не удалось распределить НДСпоОИДПБезОН по СебестоимостьОИДП: "
						+ ОтборСоединения.Организация  + ", "
						+ ОтборСоединения.ОбъектИДП    + ", "
					    + ОтборСоединения.СчетУчетаБУ  + ", "
					    + ОтборСоединения.ИмяРесурса   + ", ");
				КонецЕсли;
				
				Продолжить;
			КонецЕсли;
			
			Индекс = 0;
			
			Для Каждого СтрокаСР Из МассивСР Цикл
				СтрокаСОН = ТабНДСпоОИДПсОН.Добавить();
				ЗаполнитьЗначенияСвойств(СтрокаСОН, СтрокаБезОН);
				ЗаполнитьЗначенияСвойств(СтрокаСОН, ПустыеРесурсы);
				СтрокаСОН.ОбъектНедвижимостиСР        = СтрокаСР.ОбъектНедвижимости;
				СтрокаСОН.ВладелецОбъектаНедвижимости = СтрокаСР.ВладелецОбъектаНедвижимости;
				СтрокаСОН[ИмяРесурса]                 = Суммы[Индекс];
				СтрокаСР[ИмяРесурса]                  = СтрокаСР[ИмяРесурса] - Суммы[Индекс]; // обеспечивает списание в ноль
				
				Индекс = Индекс + 1;
			КонецЦикла;
		КонецЦикла;
	КонецЕсли;
КонецЦикла;

ТабНДСпоОИДПсОН.Свернуть("Организация, ОбъектИДП, СчетУчетаБУ, СтатьяЗатрат, ОбъектНедвижимости, ОбъектНедвижимостиСР, ВладелецОбъектаНедвижимости, СчетФактура",
	"СуммаБезНДС, НДС");

// формируем структуру данных для выгрузки
ТабДвижений = Новый ТаблицаЗначений;
ТабДвижений.Колонки.Добавить("Организация");
ТабДвижений.Колонки.Добавить("Содержание");
ТабДвижений.Колонки.Добавить("Период");	
ТабДвижений.Колонки.Добавить("ПодразделениеДт");	
ТабДвижений.Колонки.Добавить("ПодразделениеКт");
ТабДвижений.Колонки.Добавить("СчетДт");
ТабДвижений.Колонки.Добавить("СчетКт");
ТабДвижений.Колонки.Добавить("СубконтоДт");	
ТабДвижений.Колонки.Добавить("СубконтоКт");	
ТабДвижений.Колонки.Добавить("Сумма");
ТабДвижений.Колонки.Добавить("СуммаНУДт");

ТаблицыДвижений = Новый Структура;
ТаблицыДвижений.Вставить("Счет19", ТабДвижений.СкопироватьКолонки());
ТаблицыДвижений.Вставить("Счет37", ТабДвижений.СкопироватьКолонки());

СодержанияОпераций = Новый Структура;
СодержанияОпераций.Вставить("Счет19", "#НДСпоОИДП_Счет19");
СодержанияОпераций.Вставить("Счет37", "#НДСпоОИДП_Счет37");

ВидыОбъектовИДП = Новый Соответствие;

ПустойВидОбъектовИДП         = Перечисления.бит_ВидыОбъектовИДП.ПустаяСсылка();
ПустойВидОбъектаНедвижимости = Перечисления.бит_ВидыОбъектовНедвижимости.ПустаяСсылка();

СубконтоВидОбъектаИДП = Новый Структура;
СубконтоВидОбъектаИДП.Вставить("Код", "00067");

СубконтоВидОбъектаНедвижимости = Новый Структура;
СубконтоВидОбъектаНедвижимости.Вставить("Код", "00068");

СубконтоНоменклатура = ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.Номенклатура;
СубконтоСтатьиЗатрат = ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.СтатьиЗатрат;
СубконтоСчетФактуры  = ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.СФПолученные;

ДатаОперации = Параметры.ДатаОстатков - 1;

ТабДанных37 = ТабНДСпоОИДПсОН.Скопировать(, "ОбъектИДП, СчетУчетаБУ, ОбъектНедвижимостиСР, ВладелецОбъектаНедвижимости, СуммаБезНДС");
ТабДанных37.Свернуть("ОбъектИДП, СчетУчетаБУ, ОбъектНедвижимостиСР, ВладелецОбъектаНедвижимости", "СуммаБезНДС");
ТабДанных37.Колонки.Добавить("Нпп");

ТабДанных19 = ТабНДСпоОИДПсОН.Скопировать(, "ОбъектИДП, СчетУчетаБУ, СтатьяЗатрат, ОбъектНедвижимости, ОбъектНедвижимостиСР, СчетФактура, НДС");
ТабДанных19.Свернуть("ОбъектИДП, СчетУчетаБУ, СтатьяЗатрат, ОбъектНедвижимости, ОбъектНедвижимостиСР, СчетФактура", "НДС");
ТабДанных19.Колонки.Добавить("Нпп");

Нпп = 1;

Для Каждого СтрокаДанных Из ТабДанных37 Цикл
	// Счет 37
	Если СтрокаДанных.СуммаБезНДС &lt;&gt; 0 Тогда
		СчетУчетаБУ = СтрокаДанных.СчетУчетаБУ;
		
		Если Лев(СчетУчетаБУ, 3) = "37." Или Лев(СчетУчетаБУ, 3) = "43." Тогда
			ВидОбъектаНедвижимости = Параметры.СоответствиеСчетВидОН.Получить(СчетУчетаБУ);

			Если ВидОбъектаНедвижимости = Неопределено Тогда
				ВидОбъектаНедвижимости = ПустойВидОбъектаНедвижимости;
			КонецЕсли;
			
			ОбъектНедвижимости = СтрокаДанных.ОбъектНедвижимостиСР;
			
			СчетУчетаТУС2 = ""; СчетУчетаНДС2 = "";
			Выполнить(Алгоритмы.СчетУчетаТУС2_ДКП_ДДУ);
			
			СубконтоДт = Новый Соответствие;
			СубконтоДт.Вставить(СубконтоВидОбъектаНедвижимости, ВидОбъектаНедвижимости);
			
			СубконтоКт = Новый Соответствие;
			
			Движение = ТаблицыДвижений.Счет37.Добавить();
			Движение.Организация     = Параметры.Организация;                            
			Движение.Содержание      = Формат(Нпп, "ЧН=0; ЧГ=0") + "/" + СчетУчетаБУ;
			Движение.ПодразделениеДт = ?(ЗначениеЗаполнено(СтрокаДанных.ВладелецОбъектаНедвижимости), СтрокаДанных.ВладелецОбъектаНедвижимости, СтрокаДанных.ОбъектИДП);
			Движение.ПодразделениеКт = Неопределено;
			Движение.Период          = ДатаОперации;
			Движение.СчетДт          = СчетУчетаТУС2;
			Движение.СчетКт          = "00";
			Движение.СубконтоДт      = СубконтоДт;
			Движение.СубконтоКт      = СубконтоКт;
			Движение.Сумма           = СтрокаДанных.СуммаБезНДС;
			Движение.СуммаНУДт       = СтрокаДанных.СуммаБезНДС;
		КонецЕсли;
	КонецЕсли;
	
	СтрокаДанных.Нпп = Нпп;
	Нпп              = Нпп + 1;
КонецЦикла;

Нпп = 1;

Для Каждого СтрокаДанных Из ТабДанных19 Цикл
	// Счет 19
	Если СтрокаДанных.НДС &lt;&gt; 0 Тогда
		СчетУчетаБУ = СтрокаДанных.СчетУчетаБУ;
		
		ВидОбъектаНедвижимости = Параметры.СоответствиеСчетВидОН.Получить(СчетУчетаБУ);

		Если ВидОбъектаНедвижимости = Неопределено Тогда
			ВидОбъектаНедвижимости = ПустойВидОбъектаНедвижимости;
		КонецЕсли;
		
		Если СчетУчетаБУ = "36.05" Тогда
			ВидОбъектаИДП = "СКБ";
			
		ИначеЕсли СтрокаДанных.ОбъектИДП.Пустая() Тогда
			ВидОбъектаИДП = ПустойВидОбъектовИДП;
			
		Иначе
			ВидОбъектаИДП = ВидыОбъектовИДП.Получить(СтрокаДанных.ОбъектИДП);
			
			Если ВидОбъектаИДП = Неопределено Тогда
				ВидОбъектаИДП = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(СтрокаДанных.ОбъектИДП, "бит_ВидОбъекта");
				ВидыОбъектовИДП.Вставить(СтрокаДанных.ОбъектИДП, ВидОбъектаИДП);
			КонецЕсли;
		КонецЕсли;
		
		Если Лев(СчетУчетаБУ, 3) = "37." Или Лев(СчетУчетаБУ, 3) = "43." Тогда
			ОбъектНедвижимости = СтрокаДанных.ОбъектНедвижимостиСР;
			
			СчетУчетаТУС2 = ""; СчетУчетаНДС2 = "";
			Выполнить(Алгоритмы.СчетУчетаТУС2_ДКП_ДДУ);
			
		Иначе
			СчетУчетаНДС2 = Параметры.СоответствиеСчетов.Получить(СчетУчетаБУ);
			
			Если СчетУчетаНДС2 = Неопределено Тогда
				СчетУчетаНДС2 = "00";
				
			Иначе
				СчетУчетаНДС2 = Параметры.СоответствиеСчетЗатратСчетНДСТУС2.Получить(СчетУчетаНДС2);
					
				Если СчетУчетаНДС2 = Неопределено Тогда
					СчетУчетаНДС2 = "00";
				КонецЕсли;
			КонецЕсли;
		КонецЕсли;
		
