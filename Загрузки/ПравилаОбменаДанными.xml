Подразделение_ = ПараметрыАлгоритма.Подразделение; 
ТипЗначения_   = ТипЗнч(Подразделение_);

Если ТипЗначения_ = Тип("СправочникСсылка.ОбъектыСтроительства") Тогда
	ИмяПКО = "ОИДП_Подразделения";

ИначеЕсли ТипЗначения_ = Тип("СправочникСсылка.ПодразделенияОрганизаций") Тогда
	ИмяПКО = "ПодразделенияОрганизаций";
	
ИначеЕсли ТипЗначения_ = Тип("Структура") И Подразделение_.Свойство("ОбъектИДП") И Подразделение_.Свойство("Подразделение") Тогда
	ИмяПКО = "ПодразделенияИОбъектыИДП";
	
Иначе
	Отказ = Истина;	
КонецЕсли;</Текст>
