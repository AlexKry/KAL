					<Алгоритм Имя="СвойствоИсходящиеДанныеДоговорДляВзаиморасчетов" ИспользуетсяПриЗагрузке="false">
						<Текст>// Вход:  ПараметрыАлгоритма = Новый Структура("СчетТУС1*, Договор*");
// Выход: Значение, ИмяПКО, ИсходящиеДанные, Отказ;
// * - обязательный параметр

Значение = ПараметрыАлгоритма.Договор;

Если ЗначениеЗаполнено(Значение) Тогда
	ВидРасчетов_ = Строка(Параметры.СоответствиеСчетВидРасчета.Получить(ПараметрыАлгоритма.СчетТУС1));	
		
	Если ВидРасчетов_ &lt;&gt; Неопределено Тогда
		ИсходящиеДанные = Новый Структура("ВидВзаиморасчетов", ВидРасчетов_);
		
	Иначе
		Отказ = Истина;
	КонецЕсли;
	
Иначе
	Отказ = Истина;
КонецЕсли;

Если ТипЗнч(Значение) = Тип("Структура") Тогда
	ИмяПКО = "ДоговорыКонтрагентовОсновной";
КонецЕсли;</Текст>
					</Алгоритм>
					<Алгоритм Имя="ФактическиеЗатратыПоИДПСчетФакутраИмяПКО" ИспользуетсяПриЗагрузке="false">
						<Текст>ТипЗначения = ТипЗнч(ОбъектКоллекции.СчетФактура);

МэппингТиповПКО = Новый Соответствие;
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.АвансовыйОтчет")                                    , "АвансовыйОтчет_ОтражениеНДСКВычету");
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.бит_НачислениеАмортизацииОС")                       , "РегламентнаяОперация_НачислениеАмортизацииОС");  // +
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.бит_НачислениеНалоговИСборов")                      , "РегламентнаяОперация_НачислениеНалоговИСборов"); // +
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.бит_НачислениеПроцентовПоПолученнымКредитамИЗаймам"), "ПроцентовПоКредитамЗаймам_ДоходыПоФинВложениям");
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.бит_ПоступлениеОС")                                 , "ПоступлениеТоваровУслугОС");
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.бит_ПринятиеКУчетуОбъектовНедвижимости")            , "ПоступлениеТоваровУслуг_ПринятиеКУчетуОН");
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.бит_РасчетСебестоимостиОбъектовНедвижимости")       , "ОтражениеНДСКВычету_РасчетСебестоимостиОН");
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.ДокументРасчетовСКонтрагентом")                     , "ДокументРасчетовСКонтрагентом"); //ДокументРасчетовСКонтрагентом_ОтражениеНДСКВычету
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.КорректировкаПоступления")                          , "КорректировкаПоступления");
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.ОтражениеЗарплатыВРеглУчете")                       , "ОтражениеЗарплатыВБухучете");
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.ОтражениеПоступленияТоваровИУслугНДС")              , "ОтражениеНДСКВычету_ОтражениеПТиУНДС");
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.ПоступлениеДопРасходов")                            , "ПоступлениеДопРасходов");
МэппингТиповПКО.Вставить(Тип("ДокументСсылка.ПоступлениеТоваровУслуг")                           , "ПоступлениеТоваровУслуг");

Если Не ЗначениеЗаполнено(ОбъектКоллекции.СчетФактура) Тогда
	Значение = Источник;
	ИмяПКО = "ОтражениеНДСКВычетуОсновной";
	
ИначеЕсли ТипЗначения = Тип("ДокументСсылка.СчетФактураПолученный") Тогда
	ТекстЗапроса =
	"ВЫБРАТЬ
	|	СчетФактураПолученныйДокументыОснования.Ссылка КАК Ссылка,
	|	СчетФактураПолученныйДокументыОснования.ДокументОснование КАК ДокументОснование
	|ИЗ
	|	Документ.СчетФактураПолученный.ДокументыОснования КАК СчетФактураПолученныйДокументыОснования
	|ГДЕ
	|	СчетФактураПолученныйДокументыОснования.Ссылка = &amp;СчетФактура
	|
	|ОБЪЕДИНИТЬ
	|
	|ВЫБРАТЬ
	|	СчетФактураПолученный.Ссылка,
	|	СчетФактураПолученный.ДокументОснование
	|ИЗ
	|	Документ.СчетФактураПолученный КАК СчетФактураПолученный
	|ГДЕ
	|	СчетФактураПолученный.Ссылка = &amp;СчетФактура";

	Запрос    = Новый Запрос(ТекстЗапроса);
	Запрос.УстановитьПараметр("СчетФактура", ОбъектКоллекции.СчетФактура);
	Результат = Запрос.Выполнить();

	Если Результат.Пустой() Тогда
		Значение = Источник;
		ИмяПКО   = "ОтражениеНДСКВычетуОсновной";
		
	Иначе
		Значение = Результат.Выгрузить()[0].ДокументОснование;
	КонецЕсли;
	
Иначе
	ИмяПКО = МэппингТиповПКО.Получить(ТипЗначения);
	
	Если ИмяПКО = Неопределено Тогда
		ОбщегоНазначения.СообщитьОбОшибке("Не задано ПКО для " + ТипЗначения);
	КонецЕсли;
КонецЕсли;</Текст>
					</Алгоритм>
				</Группа>
				<Группа Имя="ПодборСчетов">
					<Алгоритм Имя="СчетУчетаТУС2" ИспользуетсяПриЗагрузке="false">
						<Текст>// Вход:  ПараметрыАлгоритма = Новый Структура("СчетТУС1*, Валюта");
// Выход: = ПараметрыАлгоритма.СчетТУС2*; 
// * - обязательный параметр
ПараметрыАлгоритма.Вставить("СчетТУС2", "");

Если Не ПараметрыАлгоритма.Свойство("Валюта") Тогда
	ПараметрыАлгоритма.Вставить("Валюта", "");
КонецЕсли;

Если ПараметрыАлгоритма.Валюта = "Валюта" Тогда
	ПараметрыАлгоритма.СчетТУС2 = Параметры.СоответствиеСчетовВалюта.Получить(ПараметрыАлгоритма.СчетТУС1);
	
ИначеЕсли ПараметрыАлгоритма.Валюта = "УЕ" Тогда
	ПараметрыАлгоритма.СчетТУС2 = Параметры.СоответствиеСчетовУЕ.Получить(ПараметрыАлгоритма.СчетТУС1);
КонецЕсли;

Если Не ЗначениеЗаполнено(ПараметрыАлгоритма.СчетТУС2) Или (Не ПараметрыАлгоритма.Валюта = "Валюта" И Не ПараметрыАлгоритма.Валюта = "УЕ") Тогда
	ПараметрыАлгоритма.СчетТУС2 = Параметры.СоответствиеСчетов.Получить(ПараметрыАлгоритма.СчетТУС1);
КонецЕсли;	
	
Если ПараметрыАлгоритма.СчетТУС2 = Неопределено Тогда
	ПараметрыАлгоритма.СчетТУС2 = "";
КонецЕсли;</Текст>
					</Алгоритм>
					<Алгоритм Имя="СчетУчетаТУС2_ДКП_ДДУ" ИспользуетсяПриЗагрузке="false">
						<Текст>Если Найти("37.01, 37.02, 43.03", Лев(СчетУчетаБУ, 5)) &gt; 0 Тогда 
	Если Лев(СчетУчетаБУ, 5) = "37.02" Тогда
		СчетУчетаТУС2 = "37.03";
		
	Иначе
		СчетУчетаТУС2 = Параметры.СоответствиеОН_ДКП_ДДУ.Получить(ОбъектНедвижимости);

		// По умолчанию ДКП
		Если СчетУчетаТУС2 = Неопределено Тогда
			СчетУчетаТУС2 = "37.01";
		КонецЕсли;
	КонецЕсли;
	
	СчетУчетаНДС2 = Параметры.СоответствиеСчетЗатратСчетНДСТУС2.Получить(СчетУчетаТУС2);
	
Иначе
	СчетУчетаТУС2 = "000";
	СчетУчетаНДС2 = "000";
КонецЕсли;</Текст>
					</Алгоритм>
				</Группа>
			</Группа>
			<Группа Имя="ЗаполненияПриЗагрузке">
				<Алгоритм Имя="НайтиДокументРасчетов" ИспользуетсяПриЗагрузке="true">
					<Текст>// Вход:  ПараметрыАлгоритма = Новый Структура("Организация*, Дата*, Контрагент, ДоговорКонтрагента");
// Выход: = ПараметрыАлгоритма.Документ*;
// * - обязательный параметр

ТекстЗапроса_ =
"ВЫБРАТЬ
|	ДокументРасчетовСКонтрагентом.Ссылка КАК Ссылка
|ИЗ
|	Документ.ДокументРасчетовСКонтрагентом КАК ДокументРасчетовСКонтрагентом
|ГДЕ
|	ДокументРасчетовСКонтрагентом.Организация = &amp;Организация
|	И ДокументРасчетовСКонтрагентом.Дата = &amp;Дата
|	И ДокументРасчетовСКонтрагентом.Контрагент = &amp;Контрагент
|	И ДокументРасчетовСКонтрагентом.ДоговорКонтрагента = &amp;ДоговорКонтрагента
|	И ПОДСТРОКА(ДокументРасчетовСКонтрагентом.Комментарий, 1, &amp;ДлинаКомментария) = &amp;Комментарий
|	И НЕ ДокументРасчетовСКонтрагентом.ПометкаУдаления";

Запрос_ = Новый Запрос;
Запрос_.УстановитьПараметр("Организация", ПараметрыАлгоритма.Организация);
Запрос_.УстановитьПараметр("Дата"       , ПараметрыАлгоритма.Дата);

Если ПараметрыАлгоритма.Свойство("Контрагент") Тогда
	Запрос_.УстановитьПараметр("Контрагент", ПараметрыАлгоритма.Контрагент);
	
Иначе
	ТекстЗапроса_ = СтрЗаменить(ТекстЗапроса_, "И ДокументРасчетовСКонтрагентом.Контрагент = &amp;Контрагент", "");
КонецЕсли;

Если ПараметрыАлгоритма.Свойство("ДоговорКонтрагента") Тогда
	Запрос_.УстановитьПараметр("ДоговорКонтрагента", ПараметрыАлгоритма.ДоговорКонтрагента);
	
Иначе
	ТекстЗапроса_ = СтрЗаменить(ТекстЗапроса_, "И ДокументРасчетовСКонтрагентом.ДоговорКонтрагента = &amp;ДоговорКонтрагента", "");
КонецЕсли;

Если ПараметрыАлгоритма.Свойство("Комментарий") Тогда
	Запрос_.УстановитьПараметр("ДлинаКомментария", СтрДлина(ПараметрыАлгоритма.Комментарий));
	Запрос_.УстановитьПараметр("Комментарий"     , ПараметрыАлгоритма.Комментарий);
	
Иначе
	Запрос_.УстановитьПараметр("ДлинаКомментария", СтрДлина("#ВводНачальныхОстатков"));
	Запрос_.УстановитьПараметр("Комментарий"     , "#ВводНачальныхОстатков");
КонецЕсли;

Запрос_.Текст = ТекстЗапроса_;
Результат_    = Запрос_.Выполнить();

Если Результат_.Пустой() Тогда
	Документ_ = Документы.ДокументРасчетовСКонтрагентом.СоздатьДокумент();
	ЗаполнитьЗначенияСвойств(Документ_, ПараметрыАлгоритма);
	
	Если Не ПараметрыАлгоритма.Свойство("Комментарий") Тогда
		Документ_.Комментарий = "#ВводНачальныхОстатков";
	КонецЕсли;
	
	Документ_.Записать(РежимЗаписиДокумента.Проведение);
	
Иначе
	Документ_ = Результат_.Выгрузить()[0].Ссылка;
КонецЕсли;

ПараметрыАлгоритма.Вставить("Документ",	Документ_.Ссылка);</Текст>
				</Алгоритм>
				<Алгоритм Имя="НоменклатураПослеЗагрузкиОбъекта" ИспользуетсяПриЗагрузке="true">
					<Текст>Родитель = Объект.Родитель;

Если Не Родитель.Пустая() Тогда
