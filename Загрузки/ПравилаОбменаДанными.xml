			ТЧРБП        = ТаблицаДанных.СкопироватьКолонки();
			ЗаполнитьЗначенияСвойств(ВводОстатков, СтрокаТД);
		КонецЕсли;
		
		ЗаполнитьЗначенияСвойств(ТЧРБП.Добавить(), СтрокаТД);
