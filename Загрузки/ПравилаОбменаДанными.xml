		<Группа Имя="Проверки">
			<Группа Имя="ПроверкиПриВыгрузке">
				<Алгоритм Имя="ПроверкаСчетаТУС2" ИспользуетсяПриЗагрузке="false">
					<Текст>// Вход:  ПараметрыАлгоритма = Новый Структура("СчетТУС1*, СчетТУС2, Аналитика");
// * - обязательный параметр

Если Не ПараметрыАлгоритма.Свойство("СчетТУС2") Тогда
	Выполнить(Алгоритмы.СчетУчетаТУС2);
КонецЕсли;

Если Не ЗначениеЗаполнено(ПараметрыАлгоритма.СчетТУС2) Или Лев(ПараметрыАлгоритма.СчетТУС2, 2) = "00" Тогда
	ТекстСообщения_ = "Не удалось подобрать счет ТУС 2.0 для счета ТУС 1.0 &lt;" + ПараметрыАлгоритма.СчетТУС1 + "&gt;";
	
	Если ПараметрыАлгоритма.Свойство("Аналитика") Тогда
